Import('env')
env = env.Clone()

env.LoadSoftwareUnits(level=1)

# set default script name
if env['TARGET_FAMILY'] == 'bear':
   script_fname = "smmu500.sconscript"
else:
   script_fname = "v2.sconscript"

env.SConscript(script_fname, exports='env')
