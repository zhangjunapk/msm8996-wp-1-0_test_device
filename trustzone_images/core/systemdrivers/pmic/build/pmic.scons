Import('env')

if env.has_key('MODEM_PROC'):
   env.Replace(PMIC_ROOT = [
      "${INC_ROOT}/core/systemdrivers/pmic",
   ])
   env.Append(CPPDEFINES = 'PM_IMAGE_MODEM_PROC')
